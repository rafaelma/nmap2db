import sys
import os

from nmap2db.cli import * 

__version__ = "1.0.0"

if __name__ == '__main__':

    if len(sys.argv) > 1:
        nmap2db_cli().onecmd(' '.join(sys.argv[1:]))
    elif len(sys.argv) == 1:
        os.system('clear')
        nmap2db_cli().cmdloop()
    else:
        raise NotImplementedError

